{
  "name": "@artificialsolutions/tie-api-example-ms-bot-framework",
  "version": "1.2.0",
  "description": "Example connector for Teneo and Microsoft Bot Framework",
  "author": "Lucas Willering",
  "license": "Apache-2.0",
  "main": "server.js",
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/bin/www",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/bin/www.js",
    "dev:transpile": "cross-env NODE_ENV=development npm-run-all build server",
    "dev:watch": "nodemon",
    "prod": "cross-env NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./src --out-dir dist-server",
    "clean": "rimraf dist-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artificialsolutions/tie-api-example-ms-bot-framework"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "regenerator-runtime": "^0.13.5",
    "@artificialsolutions/tie-api-client": "^1.3.5",
    "@microsoft/microsoft-graph-client": "~2.0.0",
    "botbuilder": "~4.8.0",
    "botbuilder-dialogs": "~4.8.0",
    "node-fetch": "^2.6.0",
    "restify": "^8.5.1",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@babel/node": "^7.8.7"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev:transpile",
    "watch": [
      "src/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  }
}
